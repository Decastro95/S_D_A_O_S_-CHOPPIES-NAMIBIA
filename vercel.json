{
  "version": 2,
  "builds": [
    { "src": "vite.config.js", "use": "@vercel/static-build" }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "/index.html" }
  ],
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "env": {
    "VITE_API_URL": "https://your-api-url-here",
    "VITE_API_KEY": "your-api-key-here"
  }
}
