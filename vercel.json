{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "framework": "vite",
  "env": {
    "NODE_VERSION": "22",
    "VITE_API_URL": "https://your-api-url-here",
    "VITE_API_KEY": "your-api-key-here"
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
